    <link rel="icon" href="~/MobileUX/img/faviconABC888.png" sizes="32x32" type="image/png">
    <link rel="icon" href="~/MobileUX/img/faviconABC888.png" sizes="16x16" type="image/png">

    <!-- Google fonts-->
