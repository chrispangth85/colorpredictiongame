                });

                $("#profile-mobile-menu").click();
            });

            $(".languagez").click(function (evt) {
                $.ajax({
                    type: "POST",
                    url: "/User/Languages",
                    data: { },
                    success: function (data) {
                        $("#main-content").html(data);
