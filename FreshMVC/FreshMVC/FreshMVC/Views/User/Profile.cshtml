    <script>

        $(document).ready(function () {
            $(".mainprofile").click(function (evt) {
                location.href = "/User/Profile";
            });

            $(".myredpacketz").click(function (evt) {
                $.ajax({
                    type: "POST",
                    url: "/User/MyPacketList",
                    data: {},
                    success: function (data) {
                        $("#main-content").html(data);
                    },
                    error: function (xhr, status, error) {
                        let r = JSON.parse(xhr.responseText);
                        alertify.dialog('alert').set({ title: 'Hot Mall' }).set({ transition: 'zoom', message: r.message }).show();
                    }
                });

                $("#profile-mobile-menu").click();
            });

            $(".myredpacketclaimz").click(function (evt) {
                $.ajax({
                    type: "POST",
                    url: "/User/MyPacketClaimList",
                    data: {},
                    success: function (data) {
                        $("#main-content").html(data);
                    },
                    error: function (xhr, status, error) {
                        let r = JSON.parse(xhr.responseText);
                        alertify.dialog('alert').set({ title: 'Hot Mall' }).set({ transition: 'zoom', message: r.message }).show();
                    }
                });


                $("#profile-mobile-menu").click();
            });

            $(".orderz").click(function (evt) {
                $.ajax({
                    type: "POST",
                    url: "/User/MyOrder",
                    data: {},
                    success: function (data) {
                        $("#main-content").html(data);
                    },
                    error: function (xhr, status, error) {
                        let r = JSON.parse(xhr.responseText);
                        alertify.dialog('alert').set({ title: 'Hot Mall' }).set({ transition: 'zoom', message: r.message }).show();
